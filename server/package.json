{
    "name": "labosdiscordbotv2-server",
    "version": "1.0.0",
    "description": "",
    "main": "./build/server/src/main.js",
    "bin": "./build/server/src/main.js",
    "types": "build",
    "scripts": {
        "start": "tsc -p . && ef-tspm && node ./build/server/src/main.js",
        "build": "rm -rf ./build/ && tsc -p . && ef-tspm",
        "rebuild": "npm run clean && npm i && npm run build",
        "clean": "rm -rf node_modules/ ; rm -rf ./build/",
        "auto": "npx nodemon -x npm run start -w src/ -e .ts",
        "lint": "eslint src/ --fix ; exit 0",
        "deploy": "npm run rebuild && pkg . --targets node12-linux-x64"
    },
    "private": true,
    "author": "gassastsina",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "discord-oauth2": "^2.6.0",
        "discord.js": "^12.4.1",
        "express": "^4.17.1",
        "module-alias": "^2.2.2",
        "moment": "^2.29.1",
        "mongoose": "^5.11.19",
        "socket.io": "^2.4.1"
    },
    "devDependencies": {
        "@ef-carbon/tspm": "^2.2.5",
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/mongoose": "^5.7.37",
        "@types/node": "^14.14.33",
        "@types/socket.io": "^2.1.13",
        "typescript": "^3.9.9"
    },
    "pkg": {
        "scripts": "build/**/*.js",
        "assets": "node_modules/**/*"
    }
}
